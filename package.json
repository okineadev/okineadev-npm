{
	"name": "okineadev",
	"version": "0.0.5",
	"module": "index.ts",
	"bin": {
		"okineadev": "dist/cli.js"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/okineadev/okineadev-npm.git"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/bun": "latest",
		"changelogen": "^0.5.7"
	},
	"scripts": {
		"lint": "biome check --fix",
		"format": "biome format --write .",
		"preversion": "bun run format && git add .",
		"build": "bun build ./index.ts --outfile ./dist/cli.js --target node && chmod +x ./dist/cli.js",
		"prepublishOnly": "bun run build"
	},
	"peerDependencies": {
		"typescript": "^5.0.0"
	},
	"dependencies": {
		"boxen": "^8.0.1",
		"terminal-link": "^3.0.0",
		"yoctocolors": "^2.1.1"
	}
}
